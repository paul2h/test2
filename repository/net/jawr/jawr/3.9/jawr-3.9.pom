		<license>
			<name>The Apache Software License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
			<distribution>repo</distribution>
		</license>
	</licenses>
	<scm>
		<connection>scm:git:git@github.com:ic3fox/jawr-main-repo.git</connection>
		<developerConnection>scm:git:git@github.com:ic3fox/jawr-main-repo.git</developerConnection>
		<url>http://github.com/ic3fox/jawr-main-repo</url>
		<tag>jawr-3.9</tag>
	</scm>
	<developers>
		<developer>
			<id>icefox</id>
			<name>Ibrahim Chaehoi</name>
			<roles>
				<role>Project owner</role>
			</roles>
